<Window x:Class="AssemblyInfo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:AssemblyInfo"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:command="http://www.galasoft.ch/mvvmlight"
        xmlns:views="clr-namespace:AssemblyInfo.Views"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <local:MainWindowViewModel />
    </Window.DataContext>
    <Grid Background="Transparent">

        <views:AssemblyInfoView DataContext="{Binding AssemblyInfoViewModel}" />
        <views:DropContainerView DataContext="{Binding DropContainerViewModel}" />
    </Grid>
</Window>
